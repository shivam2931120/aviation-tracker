# .env.example configuration for Aviation Tracker
# Copy this to .env and update with your values

# Database (Neon or Supabase PostgreSQL)
DATABASE_URL="postgresql://username:password@your-host.com/aviation_tracker?sslmode=require"

# Aviation APIs (exposed to frontend with VITE_ prefix)
VITE_AVIATIONSTACK_API_KEY=your_aviationstack_key
VITE_OPENSKY_CLIENT_ID=your_opensky_client_id
VITE_OPENSKY_CLIENT_SECRET=your_opensky_client_secret
VITE_FLIGHTAPI_KEY=your_flightapi_key

# Upstash Redis (optional caching)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=
