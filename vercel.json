{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "functions": {
        "api/**/*.ts": {
            "runtime": "@vercel/node@3"
        }
    }
}